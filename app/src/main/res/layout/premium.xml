<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"

    >
    <android.support.v7.widget.CardView
        android:id="@+id/card_view33"
        android:layout_width="329dp"
        android:layout_height="83dp"
        android:clickable="true"
        app:cardCornerRadius="4dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="59dp"
        android:layout_margin="40px"
        android:layout_below="@+id/card_view12"
        android:layout_centerHorizontal="true">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/shortcut"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:alpha="0" />
            <ImageView
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:scaleType="fitXY"


                android:src="@drawable/shortcut"
                android:id="@+id/imageView34" />


            <TextView
                android:id="@+id/textView34"
                android:layout_width="179dp"
                android:layout_height="wrap_content"
                android:text="SHORTCUTS"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imageView34"
                android:layout_toEndOf="@+id/imageView34"
                android:layout_marginLeft="35dp"
                android:layout_marginStart="35dp" />
        </RelativeLayout>
    </android.support.v7.widget.CardView>


    <android.support.v7.widget.CardView
        android:id="@+id/card_view34"
        android:layout_width="329dp"
        android:layout_height="83dp"
        android:clickable="true"
        app:cardCornerRadius="4dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="59dp"
        android:layout_margin="40px"
        android:layout_below="@+id/card_view12"
        android:layout_centerHorizontal="true">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/premium_tips"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:alpha="0" />
            <ImageView
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/tipsp"
                android:id="@+id/imageView35" />


            <TextView
                android:id="@+id/textView35"
                android:layout_width="179dp"
                android:layout_height="wrap_content"
                android:text="TIPS"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imageView35"
                android:layout_toEndOf="@+id/imageView35"
                android:layout_marginLeft="35dp"
                android:layout_marginStart="35dp" />
        </RelativeLayout>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/card_view35"
        android:layout_width="329dp"
        android:layout_height="83dp"
        android:clickable="true"
        app:cardCornerRadius="4dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="59dp"
        android:layout_margin="40px"
        android:layout_centerHorizontal="true">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/ask_question"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:alpha="0" />
            <ImageView
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:scaleType="fitXY"


                android:src="@drawable/ask"
                android:id="@+id/imageView36" />


            <TextView
                android:id="@+id/textView36"
                android:layout_width="179dp"
                android:layout_height="wrap_content"
                android:text="ASK A QUESTION"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imageView36"
                android:layout_toEndOf="@+id/imageView36"
                android:layout_marginLeft="35dp"
                android:layout_marginStart="35dp" />
        </RelativeLayout>
    </android.support.v7.widget.CardView>
</LinearLayout>